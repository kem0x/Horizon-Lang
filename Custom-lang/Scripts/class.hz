class IceCream 
{
	IceCream(flavor)
	{
		this.flavor = flavor;
	}	

	getFlavor()
	{
		return this.flavor;
	}
	
	serveOn()
	{
		return "Cones";
	}
};

let food: IceCream = IceCream("badFlavor");

food = IceCream("coffee");

print(food.getFlavor());

let servedOn: String = food.serveOn();

print(servedOn);

let obj: Object = {a: 1, b: 2, c: 3};

obj.a = 4;

obj = {a: 1, b: 2, c: 3, servedOn};

obj.d = 5;

let a: Number = obj.a;

let s: String = obj.servedOn;

print(a, s);
