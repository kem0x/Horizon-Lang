class IceCream 
{
	IceCream(flavor)
	{
		this.flavor = flavor;
	}	

	getFlavor()
	{
		return this.flavor;
	}
	
	serveOn()
	{
		return "Cones";
	}
};

let food: IceCream = IceCream("coffee");

let flavor: String = food.getFlavor();

print(flavor);

let servedOn: String = food.serveOn();

print(servedOn);

let obj: Object = {a: 1, b: 2, c: 3};

obj.a = 4;

obj = {a: 1, b: 2, c: 3, servedOn};

obj.d = 5;

let a: Number = obj.a;

print(a, obj);
